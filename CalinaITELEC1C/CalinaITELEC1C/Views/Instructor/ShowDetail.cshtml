@{
}
@model Instructor;
<h1>Instructor Details</h1>
<table class="table table-dark">
    <tr>
        <th>ID</th>
        <td>@Model.InstructorId</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <th>First Name</th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>Status</th>
        <td>@Model.IsTenured</td>
    </tr>
    <tr>
        <th>Hiring Date</th>
        <td>@Model.HiringDate</td>
    </tr>
    <tr>
        <th>Rank</th>
        <td>@Model.InstructorRank</td>
    </tr>
    

</table>